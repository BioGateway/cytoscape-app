<?xml version="1.0" encoding="UTF-8"?>
<form xmlns="http://www.intellij.com/uidesigner/form/" version="1" bind-to-class="eu.biogateway.cytoscape.internal.gui.BGSettingsView">
  <grid id="27dc6" binding="mainPanel" layout-manager="BorderLayout" hgap="0" vgap="0">
    <constraints>
      <xy x="20" y="20" width="680" height="115"/>
    </constraints>
    <properties/>
    <border type="none"/>
    <children>
      <grid id="b564e" layout-manager="BorderLayout" hgap="0" vgap="0">
        <constraints border-constraint="Center"/>
        <properties/>
        <border type="none" title="Custom Configuration XML Location"/>
        <children>
          <grid id="6495" layout-manager="FlowLayout" hgap="5" vgap="5" flow-align="0">
            <constraints border-constraint="South"/>
            <properties/>
            <border type="none"/>
            <children>
              <component id="fdb8c" class="javax.swing.JButton" binding="reloadConfigButton" default-binding="true">
                <constraints/>
                <properties>
                  <text value="Reload Config"/>
                </properties>
              </component>
              <component id="ced5" class="javax.swing.JButton" binding="useDefaultButton">
                <constraints/>
                <properties>
                  <text value="Use Default"/>
                </properties>
              </component>
              <component id="2e335" class="javax.swing.JButton" binding="browseButton" default-binding="true">
                <constraints/>
                <properties>
                  <text value="Browse"/>
                </properties>
              </component>
            </children>
          </grid>
          <component id="d87d1" class="javax.swing.JTextField" binding="configFileURlField">
            <constraints border-constraint="Center"/>
            <properties>
              <columns value="60"/>
              <editable value="true"/>
              <text value=""/>
            </properties>
          </component>
        </children>
      </grid>
      <grid id="7b6f8" layout-manager="BorderLayout" hgap="0" vgap="0">
        <constraints border-constraint="South"/>
        <properties/>
        <border type="none"/>
        <children>
          <component id="2f12c" class="javax.swing.JButton" binding="saveChangesButton" default-binding="true">
            <constraints border-constraint="East"/>
            <properties>
              <text value="Save changes"/>
            </properties>
          </component>
        </children>
      </grid>
    </children>
  </grid>
</form>
